<p id="notice"><%= notice %></p>

<h1>List of Shouts</h1>

<% @user.shouts.each do |shouts| %>
  <div class="shout">
    <%= shouts.body %>
<br>
    <%= link_to 'Delete', user_shout_path(@user, shouts), method: :delete %>

  </div>
<% end %>

<br>

<%= link_to 'New Shout', new_user_shout_path(@user) %>
